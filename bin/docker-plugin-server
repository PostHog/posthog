#!/bin/bash
set -e

# Leverage the node_modules directory generated by the Dockerfile
echo Syncing ./plugins/node_modules from /code/plugins/node_modules. This can take a long time the first time.
time rsync -a -W --no-compress `# https://serverfault.com/a/505758` \
  /code/plugins/node_modules/ ./plugins/node_modules

./bin/plugin-server
