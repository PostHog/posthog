{
    "name": "posthog",
    "description": "",
    "version": "0.0.0",
    "repository": {
        "type": "git",
        "url": "https://github.com/posthog/posthog.git"
    },
    "author": "PostHog Inc.",
    "bugs": {
        "url": "https://github.com/posthog/posthog/issues"
