{
    "name": "@posthog/hogvm",
    "version": "1.0.67",
    "description": "PostHog Hog Virtual Machine",
    "license": "MIT",
    "author": "PostHog <hey@posthog.com>",
    "repository": "https://github.com/PostHog/posthog",
    "source": "src/index.ts",
    "files": [
        "dist",
        "src",
        "jest.config.js",
        "tsconfig.build.json",
        "tsconfig.json",
        ".oxfmtrc.json"
    ],
    "main": "dist/index.js",
    "module": "dist/module.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "test": "jest --runInBand --forceExit",
        "clean": "rm -rf dist .parcel-cache",
        "build": "pnpm clean && pnpm build:parcel && pnpm build:cli",
        "build:parcel": "parcel build --no-cache",
        "build:compile": "tsc -p tsconfig.build.json",
        "build:cli": "mkdir -p ./dist && cp ./src/cli.js ./dist/cli.js",
        "compile:stl": "cd ../../.. && python3 -m common.hogvm.stl.compile",
        "check": "tsc -p tsconfig.build.json --noEmit",
        "format:oxfmt": "oxfmt src",
        "format:check": "oxfmt --check src",
        "prepublishOnly": "pnpm build"
    },
    "dependencies": {
        "luxon": "^3.4.4"
    },
    "devDependencies": {
        "@parcel/config-default": "2.13.3",
        "@parcel/packager-ts": "catalog:",
        "@parcel/transformer-js": "2.13.3",
        "@parcel/transformer-react-refresh-wrap": "2.13.3",
        "@parcel/transformer-typescript-types": "catalog:",
        "@swc-node/register": "^1.9.1",
        "@swc/core": "^1.11.4",
        "@swc/jest": "^0.2.37",
        "@types/jest": "catalog:",
        "@types/luxon": "^3.4.2",
        "@types/node": "catalog:",
        "crypto-browserify": "^3.12.0",
        "jest": "catalog:",
        "oxfmt": "catalog:",
        "parcel": "^2.13.3",
        "re2": "^1.23.0",
        "typescript": "catalog:"
    },
    "peerDependencies": {
        "luxon": "^3.4.4"
    }
}
