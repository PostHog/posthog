{
    "name": "Patterns App",
    "url": "https://github.com/patterns-app/posthog-patterns-app",
    "description": "Send events data to Patterns App",
    "main": "index.ts",
    "posthogVersion": ">= 1.25.0",
    "config": [
        {
            "key": "webhookUrl",
            "name": "Patterns Webhook URL",
            "type": "string",
            "required": true
        },
        {
            "key": "allowedEventTypes",
            "name": "Event types to send to Patterns (comma-separated)",
            "description": "Comma-separated like '$pageView, $pageLeave, $myEvent1'",
            "type": "string",
            "required": false
        }
    ]
}
