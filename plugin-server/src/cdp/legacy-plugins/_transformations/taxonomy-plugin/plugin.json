{
    "name": "Taxonomy Plugin",
    "url": "https://github.com/PostHog/taxonomy-plugin",
    "description": "Standardize your event names into a single pattern.",
    "main": "index.js",
    "config": [
        {
            "key": "defaultNamingConvention",
            "hint": "",
            "name": "Select your default naming pattern",
            "type": "choice",
            "choices": [
                "camelCase",
                "PascalCase",
                "snake_case",
                "kebab-case",
                "spaces in between"
            ],
            "order": 1,
            "default": "camelCase",
            "required": true
        }
    ]
}
