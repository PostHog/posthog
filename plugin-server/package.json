        "@sentry/types": "^7.17.4",
        "@swc-node/register": "^1.5.1",
        "@swc/core": "^1.2.186",
        "@swc/jest": "^0.2.21",
        "@types/adm-zip": "^0.4.34",
        "@types/babel__core": "^7.1.19",
        "@types/babel__standalone": "^7.1.4",
        "@types/faker": "^5.5.7",
        "@types/generic-pool": "^3.1.9",
        "@types/ioredis": "^4.26.4",
        "@types/jest": "^28.1.1",
        "@types/jsonwebtoken": "^8.5.5",
