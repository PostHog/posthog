    {
        "label": "Temporal Worker",
        "adapter": "Debugpy",
        "request": "launch",
        "program": "$ZED_WORKTREE_ROOT/manage.py",
        "cwd": "$ZED_WORKTREE_ROOT",
        "args": ["start_temporal_worker", "--task-queue", "development-task-queue"],
        "env": {
            "PYTHONUNBUFFERED": "1",
            "DJANGO_SETTINGS_MODULE": "posthog.settings",
            "DEBUG": "1",
            "CLICKHOUSE_SECURE": "False",
