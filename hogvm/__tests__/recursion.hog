let fibonacci := (number) -> {
    if (number < 2) {
        return number;
    } else {
        return fibonacci(number - 1) + fibonacci(number - 2);
    }
}

print(fibonacci(6))

fn fibonacci2(number) {
    if (number < 2) {
        return number;
    } else {
        return fibonacci(number - 1) + fibonacci(number - 2);
    }
}

print(fibonacci2(6))

