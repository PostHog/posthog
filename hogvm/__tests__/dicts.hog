fn test(val) {
  print(jsonStringify(val));
}

test({});
test({'key': 'value'});
test({'key': 'value', 'other': 'thing'});
test({'key': {'otherKey': 'value'}});
test({key: 'value'});

let key := 3;
test({key: 'value'});

test({'key': 'value'}.key);
test({'key': 'value'}['key']);
test({'key': {'otherKey': 'value'}}.key.otherKey);
test({'key': {'otherKey': 'value'}}['key'].otherKey);
