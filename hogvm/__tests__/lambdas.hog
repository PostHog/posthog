let b := x -> x * 2
print(b)
print(b(2))
print(b(8))

print('--------')

let func := x -> x * 2
let arr := [func]

print(func(2))
print(arr[1](2))
print((x -> x * 2)(2))

print('--------')

let withArg := x -> {
    print(x)
    print('moo')
    print('cow')
}
withArg(2)

print('--------')

let noArg := () -> {
    print('moo')
    print('cow')
}
noArg()

