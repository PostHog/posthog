{
    "name": "@posthog/playwright",
    "scripts": {
        "build:products": "pnpm --filter=@posthog/frontend build:products",
        "pretest": "pnpm build:products && pnpm --filter=@posthog/playwright exec playwright install",
        "test": "playwright test",
        "test:local": "BASE_URL='http://localhost:8000' pnpm run test",
        "test:ui": "pnpm run test --ui",
        "test:ui:local": "BASE_URL='http://localhost:8000' pnpm run test:ui"
    },
    "dependencies": {
        "@playwright/test": "1.45.0",
        "@posthog/frontend": "workspace:*"
    },
    "peerDependencies": {}
}
