# name: TestPerson.test_filter_person_email
  '
  /* user_id:0 request:_snapshot_ */
  SELECT id
  FROM person
  WHERE team_id = 2
  GROUP BY id
  HAVING max(is_deleted) = 0
  AND has(['another@gmail.com'], replaceRegexpAll(JSONExtractRaw(properties, 'email'), '^"|"$', ''))
  ORDER BY max(created_at) DESC, id
  LIMIT 100
  '
---
