# name: TestFeatureFlagMatcher.test_multiple_flags.1
  '
  SELECT ("posthog_person"."properties" -> 'email') = '"test@posthog.com"' AS "flag_29_condition_0",
         (true) AS "flag_29_condition_1",
         (true) AS "flag_30_condition_0",
         (true) AS "flag_31_condition_0",
         (true) AS "flag_35_condition_0"
  FROM "posthog_person"
  INNER JOIN "posthog_persondistinctid" ON ("posthog_person"."id" = "posthog_persondistinctid"."person_id")
  WHERE ("posthog_persondistinctid"."distinct_id" = 'test_id'
         AND "posthog_persondistinctid"."team_id" = 2
         AND "posthog_person"."team_id" = 2)
  '
---
# name: TestFeatureFlagMatcher.test_multiple_flags.2
  '
  SELECT ("posthog_group"."group_key" = 'group_key'
          AND "posthog_group"."group_type_index" = 2) AS "flag_32_condition_0",
         ("posthog_group"."group_key" = 'group_key'
          AND "posthog_group"."group_type_index" = 2) AS "flag_33_condition_0",
         ("posthog_group"."group_key" = 'foo'
          AND "posthog_group"."group_type_index" = 2) AS "flag_34_condition_0"
  FROM "posthog_group"
  WHERE "posthog_group"."team_id" = 2
  '
---
