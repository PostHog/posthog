           FROM
             (SELECT toStartOfWeek(toDateTime('2020-01-04 23:59:59', 'UTC'), 0) - toIntervalWeek(number) as day_start
              FROM numbers(2)
              UNION ALL SELECT toStartOfWeek(toDateTime('2019-12-28 00:00:00', 'UTC'), 0) as day_start) as ticks
           CROSS JOIN
