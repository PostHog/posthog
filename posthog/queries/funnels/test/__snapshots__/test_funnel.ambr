                           OR step_1 = 1) ) ARRAY
                 JOIN prop_vals as prop
                 WHERE prop != [] ))
           WHERE step_0 = 1 SETTINGS allow_experimental_window_functions = 1 ))
     GROUP BY aggregation_target,
              steps,
              prop
     HAVING steps = max_steps SETTINGS allow_experimental_window_functions = 1)
