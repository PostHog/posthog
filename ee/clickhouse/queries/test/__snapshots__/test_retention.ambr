         datediff('Week', reference_event.event_date, toStartOfWeek(toDateTime(event_date))) as intervals_from_base,
         COUNT(DISTINCT event.target) count
  FROM
    (SELECT e.timestamp AS event_date,
            pdi.person_id as target,
            e.uuid AS uuid,
            e.event AS event
     FROM events e
