               product_id AS product_id,
               toString(person_id) AS customer_id,
               NULL AS status,
               min_timestamp AS started_at,
               if(ifNull(greater(max_timestamp_plus_dropoff_days, today()), 0), NULL, max_timestamp) AS ended_at,
