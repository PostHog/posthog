        "clsx": "^2.0.0",
        "lucide-react": "^0.539.0",
        "recharts": "^2.8.0",
        "tailwind-merge": "^2.0.0",
        "zod": "^3.22.4"
    },
    "devDependencies": {
        "@playwright/test": "^1.40.0",
        "@rollup/plugin-commonjs": "^25.0.0",
        "@rollup/plugin-node-resolve": "^15.0.0",
