# PostHog Feature Flags Testing Configuration
# Copy this file to .env and fill in your values

# PostHog host URL (default: http://localhost:8010 for local development)
POSTHOG_HOST=http://localhost:8010

# Rust feature flags service URL (default: http://localhost:3001)
POSTHOG_RUST_SERVICE_HOST=http://localhost:3001

# Personal API Key (required for flag management)
# Generate at: http://localhost:8010/settings/user-api-keys
POSTHOG_PERSONAL_API_KEY=

# Project API Key (required for flag evaluation)
# Find at: http://localhost:8010/settings/project#variables
POSTHOG_PROJECT_API_KEY=
